<tr>
  <td><%= question.text %></td>
  <td class="text-center">
    <%= link_to edit_question_path(question) do %>
      <span class="glyphicon glyphicon-pencil"></span>
    <% end %>
  </td>
  <td>
    <%= link_to question,  method: :delete, data: { confirm: 'Tem certeza?' } do %>
    <span class="glyphicon glyphicon-trash"></span>
    <% end %>
  </td>
</tr>
